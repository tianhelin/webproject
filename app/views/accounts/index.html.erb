<h1>帳號管理</h1>
<table class="table">
    <td>使用者名稱</td>
    <td>電子信箱</td>
    <td>生日</td>
    <td>管理權限</td>
  <tr>
  <% @accounts.each do |account| %>
    <td>
      <%= account.name %>
    </td>
    <td>
      <%= account.email %>
    </td>
    <td>
      <%= account.birthday %>
    </td>
    <td>
      <%= account.adminkey %>
    </td>
  </tr>
  <% end %>
</table>
