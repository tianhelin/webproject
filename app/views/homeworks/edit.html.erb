<div class="xs-60-container homework">
  <h2 class="welcome-title">
    <%= '編輯：' %>
    <%= @homework.title %>
  </h2>
  
  <%= form_for @homework do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= f.submit "更新" ,:class => 'btn btn-primary pull-right'%>
  <div class="clearfix"></div>
  <% end %>
  <%= link_to sanitize('<i class="fa fa-arrow-circle-left" aria-hidden="true"></i>'),homeworks_path , :class => "btn btn-back" %>
</div>